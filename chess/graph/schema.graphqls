type Puzzle {
	id: String!
	fen: String!
	moves: String!
	rating: Int!
	popularity: Int!
	themes: String!
}

type Query{
  initPuzzles(input: PuzzleInput!): [Puzzle!]!
}

input PuzzleInput{
  limit:Int!
  room:String!
}
type MutationReply{
  msg:String!
}
type Mutation  {
	getPuzzles(input: PuzzleInput!): MutationReply!
}

type Subscription{
    latestPuzzles(input: PuzzleInput!): [Puzzle!]!
}